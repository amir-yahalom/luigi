    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.5",
    "jest": "^26.0.1",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^25.5.1",
    "tslint": "^6.1.2",
